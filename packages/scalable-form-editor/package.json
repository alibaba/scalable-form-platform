{
  "name": "scalable-form-editor",
  "version": "0.1.0",
  "description": "通过ScalableFormEditor可视化编排表单",
  "homepage": "",
  "license": "ISC",
  "main": "src/index.js",
  "typings": "src/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "dev": "exit 0",
    "start": "exit 0",
    "stop": "exit 0",
    "build": "npm run build:dir && npm run build:js && npm run build:copy",
    "publish": "npm run build:dir && npm run build:js && node ./prepare.js && npm publish build",
    "test": "jest --updateSnapshot --coverage",
    "build:dir": "rimraf ./build && mkdir build",
    "build:js": "cross-env NODE_ENV=production webpack --config ./webpack.config.babel.js --colors",
    "build:copy": "mkdir ../../dist/scalable-form-editor && ls ./build/* && cp ./build/* ../../dist/scalable-form-editor/"
  },
  "dependencies": {
    "scalable-form-antd": "^0.1.0",
    "scalable-form-core": "^0.1.0",
    "antd": "^3.24.2",
    "classnames": "^2.2.6",
    "natty-fetch": "^2.5.3",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^16.8.6",
    "react-if": "^2.2.1",
    "react-redux": "^4.4.8",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^6.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "6.6.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-react": "7.16.0",
    "jest": "^24.7.1",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "jest-simple-summary": "^1.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "verbose": true,
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/test-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "((\\.|/)(test|spec))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^.*[.](jpg|JPG|gif|GIF|png|PNG|less|LESS|css|CSS)$": "<rootDir>/test/EmptyModule"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/test/EmptyModule.js"
    ]
  }
}
