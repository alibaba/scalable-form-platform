{
  "name": "scalable-form-antd-mobile",
  "version": "0.2.0",
  "description": "A react component for rendering web forms in mobile browser from JSON schema using ant design mobile components",
  "homepage": "https://scalable-form-platform.github.io",
  "keywords": [
    "mobile",
    "javascript",
    "react",
    "antd-mobile",
    "ant-design-mobile",
    "json-schema",
    "form",
    "ui",
    "ui-components",
    "json",
    "solution",
    "sdk"
  ],
  "bugs": "https://github.com/alibaba/scalable-form-platform/issues",
  "repository": "https://github.com/alibaba/scalable-form-platform",
  "license": "MIT",
  "main": "src/index.tsx",
  "typings": "src/index.tsx",
  "author": {
    "name": "alibaba"
  },
  "contributors": [
    "吕舜 lvshun@live.cn",
    "Ma Yongxin"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "dev": "exit 0",
    "start": "exit 0",
    "stop": "exit 0",
    "build": "npm run build:dir && npm run build:js && npm run build:copy",
    "test": "jest --updateSnapshot --coverage --no-cache",
    "publish": "npm run build:dir && npm run build:js && node ./prepare.js && npm publish build",
    "build:dir": "rimraf ./build && mkdir build",
    "build:js": "cross-env NODE_ENV=production webpack --config ./webpack.config.babel.js --colors",
    "build:copy": "mkdir ../../dist/scalable-form-antd-mobile && ls ./build/* && cp ./build/* ../../dist/scalable-form-antd-mobile/"
  },
  "dependencies": {
    "antd-mobile": "^2.3.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "scalable-form-core": "^0.2.0",
    "scalable-form-tools": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/lodash": "^4.14.150",
    "@types/react-dom": "^16.8.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^6.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "6.6.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-react": "7.16.0",
    "jest": "^24.7.1",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "jest-simple-summary": "^1.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    },
    "verbose": true,
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/test-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^.*[.](jpg|JPG|gif|GIF|png|PNG|less|LESS|css|CSS)$": "<rootDir>/test/EmptyModule"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/test/EmptyModule.js"
    ]
  }
}
