{
  "name": "scalable-form-server",
  "version": "0.0.15",
  "description": "Scalable Form server sdk, providing koa/express middleware to apply Scalable Form web site",
  "homepage": "https://scalable-form-platform.github.io",
  "keywords": [
    "javascript",
    "react",
    "antd",
    "ant-design",
    "json-schema",
    "form",
    "ui",
    "ui-components",
    "json",
    "solution",
    "sdk",
    "koa",
    "express",
    "egg"
  ],
  "bugs": "https://github.com/alibaba/scalable-form-platform/issues",
  "repository": "https://github.com/alibaba/scalable-form-platform",
  "license": "MIT",
  "main": "build/index.js",
  "typings": "build/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "author": {
    "name": "alibaba"
  },
  "contributors": [
    "吕舜 lvshun@live.cn",
    "Ma Yongxin"
  ],
  "files": [
    "build",
    "standalone"
  ],
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'npm run dev:build && node ./build/standalone.js'",
    "dev:build": "cross-env NODE_ENV=development webpack --config webpack.config.babel.js --colors",
    "start": "exit 0;",
    "build": "npm run build:dir && npm run build:pack && npm run build:copy",
    "build:dir": "rimraf ./build && mkdir build",
    "build:pack": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js --colors",
    "build:copy": "mkdir ../../dist/scalable-form-server && ls ./build/* && cp ./build/* ../../dist/scalable-form-server/",
    "test": "exit 0;",
    "publish": "npm run build:dir && npm run build:pack && node ./prepare.js && npm publish build"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@types/koa": "^2.11.3",
    "@types/mysql": "^2.15.7",
    "@types/redis": "^2.8.14",
    "chalk": "^3.0.0",
    "co-body": "^6.0.0",
    "commander": "^4.0.1",
    "cross-env": "^6.0.3",
    "ejs": "^2.7.1",
    "koa": "^2.11.0",
    "mysql": "^2.17.1",
    "node-xlsx": "^0.15.0",
    "redis": "^2.8.0",
    "sqlite3": "^4.1.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.4.0",
    "@types/expect": "^1.20.4",
    "@types/jest": "^24.0.11",
    "babel-loader": "^8.0.6",
    "jest": "^24.7.1",
    "jest-simple-summary": "^1.0.1",
    "nodemon": "^1.19.4",
    "ts-jest": "^24.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageReporters": [
      "clover",
      "html"
    ],
    "reporters": [
      "default",
      "jest-simple-summary"
    ],
    "collectCoverage": true
  }
}
