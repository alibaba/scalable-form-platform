{
  "name": "scalable-form-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn workspace scalable-form-sdk run dev",
    "dev:server": "lerna run --parallel dev --scope scalable-form-server --scope scalable-form-admin",
    "prebuild": "node_modules/.bin/rimraf ./dist && mkdir dist",
    "build": "lerna run --parallel build",
    "start": "lerna run --stream start --scope scalable-form-server",
    "stop": "lerna run --stream stop",
    "test": "lerna run --parallel test --scope scalable-form-core",
    "test:core": "lerna run --parallel test --scope scalable-form-core",
    "test:antd-mobile": "lerna run --parallel test --scope scalable-form-antd-mobile",
    "test:tools": "lerna run --parallel test --scope scalable-form-tools",
    "test:editor": "lerna run --parallel test --scope scalable-form-editor",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --ext .ts,.tsx packages",
    "lint:style": "stylelint 'packages/**/*.less'",
    "lint:codefix": "eslint --ext .ts,.tsx packages --fix",
    "lint:stylefix": "stylelint 'packages/**/*.less' --fix",
    "type:check": "tsc --noEmit",
    "publish": "lerna run --parallel publish",
    "publish:daily": "node ./scripts/version-daily.js && npm run publish",
    "publish:core": "lerna run --parallel publish --scope scalable-form-core",
    "publish:antd": "lerna run --parallel publish --scope scalable-form-antd",
    "publish:antd-mobile": "lerna run --parallel publish --scope scalable-form-antd-mobile",
    "publish:editor": "lerna run --parallel publish --scope scalable-form-editor",
    "publish:server": "lerna run --parallel publish --scope scalable-form-server"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@storybook/react": "^5.3.19",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cache-loader": "^4.1.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-ali": "^9.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-tsdoc": "^0.2.4",
    "git-branch": "^2.0.1",
    "husky": "^1.3.1",
    "lerna": "^3.18.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "thread-loader": "^2.1.3",
    "typescript": "^3.4.4",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "workspaces": [
    "packages/scalable-form-core",
    "packages/scalable-form-tools",
    "packages/scalable-form-antd",
    "packages/scalable-form-antd-mobile",
    "packages/scalable-form-editor",
    "example/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "post-checkout": "node ./scripts/version.js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "__npminstall_done": false
}
